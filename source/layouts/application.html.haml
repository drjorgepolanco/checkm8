!!!
%html
  %head
    %meta{charset: "utf-8"}
    %meta{content: "IE=edge,chrome=1", http: {equiv: "X-UA-Compatible"}}
    %meta{name: "viewport", content: "width=device-width, initial-scale=1"}
    %title= current_page.data.title || "Coach Swagger"
    = stylesheet_link_tag "http://vjs.zencdn.net/4.12/video-js.css"
    = stylesheet_link_tag "application"

  %body{class: page_classes}
    = yield
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"
    = javascript_include_tag "http://vjs.zencdn.net/4.12/video.js"
    = javascript_include_tag "vendor/slick"
    = javascript_include_tag "main"
